use warnings;
use strict;

print "51 ANDed with 85 gives us ", 51 & 85, "\n";
print "51 ANDed with 170 gives us ", 51 & 170, "\n";
print "204 ORed with 85 gives us ", 204 | 85, "\n";
print "204 ^ORed with 170 gives us ", 204 ^ 170, "\n";
print "NOT 85 ", ~85, "\n";
